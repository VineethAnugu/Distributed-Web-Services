<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.teja.vin.com/service"
targetNamespace="http://teja.vin.com/service" elementFormDefault="qualified">
 
    <xs:element name="AddRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="num1" type="xs:float"/>
                <xs:element name="num2" type="xs:float"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
 
     <xs:element name="MinusRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="num1" type="xs:float"/>
                <xs:element name="num2" type="xs:float"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="Response">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="res_num" type="xs:float"/>
                <xs:element name="comment" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="WhichRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceName" type="xs:string"/>
                <xs:element name="load_inc" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="WhichResponse">
        <xs:complexType>
            <xs:sequence>
			<xs:element name="Server" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="AliveRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="IPAddress" type="xs:string"/>
                <xs:element name="port" type="xs:int"/>
                <xs:element name="serviceNames" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="AliveResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Status" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>     
    
        <xs:element name="DeadRequest">
	        <xs:complexType>
	            <xs:sequence>
	                <xs:element name="IPAddress" type="xs:string"/>
	                <xs:element name="port" type="xs:int"/>
	            </xs:sequence>
	        </xs:complexType>
    	</xs:element>
    	
    	    <xs:element name="DeadResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Comment" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>